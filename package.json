{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:python\"",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "bash build-for-deploy.sh",
    "dev:next": "wait-on tcp:8000 && next dev -p 5000 -H 0.0.0.0",
    "dev:python": "sh -c 'cd python_service && python main.py'",
    "start:next": "next start -p ${PORT:-5000} -H 0.0.0.0",
    "start:python": "cd python_service && python main.py",
    "start": "bash start-services.sh",
    "start:concurrent": "concurrently \"npm run start:next\" \"npm run start:python\"",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
